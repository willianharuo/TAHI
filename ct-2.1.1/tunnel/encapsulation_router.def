	v6_echo_request_LINK0_tn2nut_OFFLINK,
	_HETHER_tn2nut,
		{
		_SRC(TN_LINK0_GLOBAL_ADDRESS);
		_DST(OFFLINK_GLOBAL_ADDRESS);
	},
		{
		payload=ping_data;
	}
)


