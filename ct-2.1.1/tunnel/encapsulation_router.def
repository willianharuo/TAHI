	v6overv4_echo_request_LINK1_nut2tn,
	_HETHER_nut2tn,
	NUT_LINK1_V4_ADDRESS,
	V4_OFFLINK0_ADDRESS,
	 	{
		_SRC(TN_LINK0_GLOBAL_ADDRESS);
		_DST(OFFLINK_GLOBAL_ADDRESS);
	},
		{
		payload=ping_data;
	}
)



