#include "icmp.def"

FEM_icmp6_echo_request(
	echo_request_tn2nut_source_LLA_destination_GA,
	_HETHER_tn2nut,
		{
                _SRC(tnv6()); 
                _DST(NUT_GLOBAL_ADDRESS); 
	},
		{
                payload=ping_data; 
	}
)

FEM_icmp6_destination_unreachable(
	destination_unreachable_nut2tn_any_code_two,
	_HETHER_nut2tn,
		{
		_SRC(NUT_ONEOF_ADDRESSES);
		_DST(TN_ONEOF_ADDRESSES);
	},
		{
		Code = 2;
		payload = any;
	}
)

