#define AALGO_FROM_HOST1	aalgo_hmacmd5_012
#define AALGO_TO_HOST1		aalgo_hmacmd5_012

#define IPSEC_ECHO_DATA		echo_data_1

#include "./HTR_A_common.def"

//======================================================================
//
// AH definition
//
Hdr_AH ah_from_host1_2 {
	SPI = 0x2000;
	SequenceNumber = 1;
	algorithm = aalgo_hmacmd5_foo;
}

Hdr_AH ah_to_host1_2 {
	SPI = 0x2000;
	SequenceNumber = any; #1;
	algorithm = aalgo_hmacmd5_foo;
}

//======================================================================
//
// Echo Request from Host1 IPsec AH
//

FEM_icmpX_echo_request_exth(
	echo_request_from_host1_ah_2,
	hether_router2nut,
	{
		_SRC(vX(IPSEC_HOST1_NET5_ADDR));
		_DST(vX(IPSEC_NUT_NET3_ADDR));
	},
	{
		header = _HDR_IPVX_NAME(echo_request_from_host1_ah_2);
		exthdr = ah_from_host1_2;
		upper = _ICMPVX_NAME(echo_request_from_host1_ah_2);
	},
	{
		payload = echo_data_2;
	}
)

//======================================================================
//
// Echo Reply to Host1 IPsec AH (SPI=0x2000)
//

FEM_icmpX_echo_reply_exth(
	echo_reply_to_host1_ah_2,
	hether_nut2router,
	{
		_SRC(vX(IPSEC_NUT_NET3_ADDR));
		_DST(vX(IPSEC_HOST1_NET5_ADDR));
	},
	{
		header = _HDR_IPVX_NAME(echo_reply_to_host1_ah_2);
		exthdr = ah_to_host1_2;
		upper = _ICMPVX_NAME(echo_reply_to_host1_ah_2);
	},
	{
		payload = echo_data_2;
	}
)

//======================================================================
//
// Echo Request from Host1
//

FEM_icmpX_echo_request(
	echo_request_from_host1_2,
	hether_router2nut,
	{
		_SRC(vX(IPSEC_HOST1_NET5_ADDR));
		_DST(vX(IPSEC_NUT_NET3_ADDR));
	},
	{
		payload = echo_data_2;
	}
)

//======================================================================
//
// Echo Reply to Host1
//

FEM_icmpX_echo_reply(
	echo_reply_to_host1_2,
	hether_nut2router,
	{
		_SRC(vX(IPSEC_NUT_NET3_ADDR));
		_DST(vX(IPSEC_HOST1_NET5_ADDR));
	},
	{
		payload = echo_data_2;
	}
)

//end


