#define EALGO_FROM_HOST1	ealgo_null_hmacsha2_256_012
#define EALGO_TO_HOST1		ealgo_null_hmacsha2_256_012

#define ESP_FROM_HOST1		esp_from_host1_pad
#define ESP_TO_HOST1		esp_to_host1_pad

ESPAlgorithm ealgo_null_hmacsha2_256_012_pad {
	crypt = null_crypt(4);
	auth = hmacsha2_256("TAHITEST89ABCDEFTAHITEST89ABCDEF");
	pad = sequential();
}

Hdr_ESP esp_to_host1_pad {
	SPI = 0x1000;
	SequenceNumber = any;	// 1;
	algorithm = ealgo_null_hmacsha2_256_012_pad;
	PadLength = 3;
}

Hdr_ESP esp_from_host1_pad {
	SPI = 0x1000;
	SequenceNumber = 1;
	algorithm = ealgo_null_hmacsha2_256_012_pad;
	PadLength = 3;
}

#define IPSEC_ECHO_DATA echo_data_odd

